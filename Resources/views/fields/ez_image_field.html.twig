{% block ezimage_field %}
{% apply spaceless %}
    {% set default_parameters = {
        'class': '',
        'alt': '',
        'parent-fit': 'cover',
        'object-fit': 'cover',
        'caption': '',
        'captionFieldIdentifier': 'caption'
    } %}
    {% set default_config = { 'default': parameters.alias|default('ez_progressive_admin') } %}
    {% set parameters = parameters|merge(default_parameters) %}
    {% include '@EzProgressiveImage/image.html.twig' with {
        'config': parameters.config|default(default_config),
        'identifier': field.fieldDefIdentifier
    } %}
{% endapply %}
{% endblock %}
